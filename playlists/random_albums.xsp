<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
    <name>Random albums</name>
    <match>all</match>
	<rule field="lastplayed" operator="notinthelast">
   		<value>30 days</value>
	</rule>
    <limit>4</limit>
    <order direction="ascending">random</order>
</smartplaylist>
